<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#00c8c8c8" android:endColor="#00e6e6e6" android:angle="270.0" android:centerColor="#00e3e3e3" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="@color/colorProgressBar" android:endColor="@color/colorProgressBar" android:centerColor="@color/colorProgressBar" />
            </shape>
        </clip>
    </item>
</layer-list>